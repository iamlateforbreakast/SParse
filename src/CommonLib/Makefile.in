.c.o:
	@CC@ @CFLAGS@ $< -o $@

LIB = libCommon

SOURCES = \
	./Object/Object.c \
	./List/List.c \
	./Map/Map.c 

HEADERS = $(SOURCES:.c=.h)

OBJECTS = $(SOURCES:.c=.o)

all: libCommon.a

libCommon.a: includes $(OBJECTS)

	@MAKELIB@ $(OBJECTS)
	@RANLIB@ $@

includes:
	mkdir -p include
	for f in $(HEADERS); do \
	    ln -sf .$$f -t include; \
	done
	
clean:
	@CLEAN@ include $(OBJECTS) libCommon.a

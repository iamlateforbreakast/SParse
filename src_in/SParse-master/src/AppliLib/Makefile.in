.c.o:
	@CC@ @CFLAGS@ $< -o $@

LIB = libAppli

SOURCES = \
	./ErrorMgr/ErrorMgr.c \
	./FileMgr/FileMgr.c \
	./OptionMgr/OptionMgr.c \
	./TimeMgr/TimeMgr.c \
	./ObjectMgr/ObjectMgr.c

HEADERS = $(SOURCES:.c=.h)

OBJECTS = $(SOURCES:.c=.o)

all: libAppli.a

libAppli.a: $(OBJECTS)
	mkdir -p include
	ln -sf $(HEADERS) include
	@MAKELIB@ $(OBJECTS)
	@RANLIB@ $@

clean:
	@CLEAN@
